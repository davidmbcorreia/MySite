                                        <input type="text" class="form-control" placeholder="Last Name" name="last_name" required="required" />
                                    </div>
                                    <div class="control-group col-lg-6">
                                        <input type="text" class="form-control" placeholder="Company Name" name="company" required="required" />
                                        <input type="email" class="form-control" placeholder="Email" name="email" required="required"/>
                                        <input type="phone" class="form-control" placeholder="Phone" name="phone" />
                                    </div>
                                    <div class="control-group">
                                        <div class="form-group col-xs-12 floating-label-form-group controls">
                                            <textarea rows="6" class="form-control" placeholder="Message" name="description" required="required"></textarea>
                                        </div>
                                    </div>
